class Animal {
    constructor (name){
     
        this.name = name
    }
  
    diets(){
        console.log ("it is unknown if" + this.name + " is a carnivore");
    }
}
   

class Human extends Animal {
   /* constructor(name) {
        let human = new Human ("human")
    } */
    diets(){
        console.log ("it is false that" + this.name + " is a carnivore");
    }
        
    }
  


class Lion extends Animal {
    /*
    constructor(name) {
        let tiger = new Tiger ("tiger")
    }
*/
    diets(){
        super.diets();
    console.log ("it is true that" + this.name + " is a carnivore");
    }
    }

    let simba = new Lion(" simba");
    simba.diets();